{
	"compilerOptions": {
		"lib": [
			"es5",
			"es6",
			"dom",
			"es2015.core",
			"es2015.collection",
			"es2015.generator",
			"es2015.iterable",
			"es2015.promise",
			"es2015.proxy",
			"es2015.reflect",
			"es2015.symbol",
			"es2015.symbol.wellknown",
			"esnext.asynciterable"
		],
		"module": "commonjs",
		"target": "es6",
		"jsx": "react-native",
		"strict": true,
		"moduleResolution": "node",
		"sourceMap": false,
		"outDir": "dist",
		"baseUrl": ".",
		"paths": {
			"*": ["node_modules/*", "types/*"]
		}
	},
	"include": ["pages/**/*", "src/**/*.ts", "src/**/*.tsx"]
}
